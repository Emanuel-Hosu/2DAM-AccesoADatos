package ArrayToStack;

import java.util.ArrayList;

public class Stack<T> {
	private ArrayList<T> stack;
	
	public Stack() {
		stack = new ArrayList<T>();
	}
	
	public void push(T elem) {
		stack.add(elem);
	}
	
	public int pop() {
		int remover = stack.size() - 1;
		stack.remove(stack.size() - 1);
	}
	
}
